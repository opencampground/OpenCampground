<table>
  <tr>
    <th><%= I18n.t('user.Name')%></th>
    <th><%= I18n.t('user.Admin')%></th>
  </tr>

<% for user in @users %>
  <tr>
    <td><%=h user.name %></td>
    <td><%=h user.admin %></td>
    <% if user.id != @user_login.id %>
      <td><%= link_to I18n.t('user.Change'),  edit_user_url(user) %></td>
      <td><%= link_to I18n.t('user.Remove'), user, :confirm => I18n.t('user.ConfirmRemove', :name => user.name), 
				       :method => :delete %></td>
    <% end %>
  </tr>
<% end %>
</table>

<br />

<%= link_to I18n.t('user.New'), new_user_path %> &nbsp;
<%= link_to I18n.t('user.Finished'), :controller => 'reservation', :action => 'list' %>
